{
  "expo": {
    "name": "FitLog",
    "slug": "fitlog-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "fitlog",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#09090B"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.seckinsoygan.fitlog",
      "buildNumber": "2",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Bu uygulama kamera kullanmaz.",
        "NSPhotoLibraryUsageDescription": "Bu uygulama fotoÄŸraf galerisi kullanmaz.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#09090B"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.seckinsoygan.fitlog",
      "versionCode": 2,
      "permissions": [
        "INTERNET",
        "VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "name": "FitLog",
      "shortName": "FitLog",
      "description": "Your personal fitness tracking companion",
      "backgroundColor": "#09090B",
      "themeColor": "#D4FF00",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "scope": "/"
    },
    "plugins": [
      "expo-sqlite",
      "expo-web-browser",
      "expo-apple-authentication"
    ],
    "extra": {
      "eas": {
        "projectId": "a663ab9d-f85d-4e7d-b8f9-67cf4f32ad14"
      }
    },
    "owner": "seckinsoygan"
  }
}
